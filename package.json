{
  "name": "formulr",
  "version": "0.2.0-beta.31",
  "description": "Form toolkit for React",
  "main": "./lib/index.js",
  "module": "./esm/index.js",
  "types": "./esm/index.d.ts",
  "scripts": {
    "dev": "webpack-dev-server --progress",
    "build:esm": "rimraf ./esm && tsc --module es2015 --outDir esm",
    "build:cmd": "rimraf ./cmd && tsc --module commonjs --outDir cmd",
    "build": "npm run build:esm && npm run build:cmd",
    "prepublishOnly": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/intellild/formulr.git"
  },
  "keywords": [
    "react",
    "form"
  ],
  "files": [
    "src/**/*",
    "cmd/**/*",
    "esm/**/*"
  ],
  "author": "intellild",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/intellild/formulr/issues"
  },
  "homepage": "https://github.com/intellild/formulr#readme",
  "dependencies": {
    "@types/big.js": "^4.0.5",
    "@types/react": "^16.8.0",
    "@types/react-dom": "^16.8.0",
    "big.js": "^5.2.2",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "rxjs": "^6.4.0",
    "tslib": "^1.9.3",
    "utility-types": "^3.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.0",
    "@babel/preset-react": "^7.0.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.6",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.8.0",
    "rimraf": "^3.0.0",
    "typescript": "~3.4.0",
    "webpack": "^4.35.2",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.7.2"
  },
  "peerDependencies": {
    "scheduler": "~0.14.0 || ~0.15.0"
  }
}

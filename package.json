{
  "name": "formulr",
  "version": "0.2.0-beta.21",
  "description": "Form toolkit for React",
  "main": "./lib/index.js",
  "module": "./esm/index.js",
  "types": "./esm/index.d.ts",
  "scripts": {
    "dev": "docz dev",
    "build:esm": "rimraf ./esm && tsc --module es2015 --outDir esm",
    "build:cmd": "rimraf ./cmd && tsc --module commonjs --outDir cmd",
    "build": "npm run build:esm && npm run build:cmd",
    "prepublishOnly": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/intellild/formulr.git"
  },
  "keywords": [
    "react",
    "form"
  ],
  "files": [
    "src/**/*",
    "cmd/**/*",
    "esm/**/*"
  ],
  "author": "intellild",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/intellild/formulr/issues"
  },
  "homepage": "https://github.com/intellild/formulr#readme",
  "dependencies": {
    "@types/big.js": "^4.0.5",
    "@types/react": "^16.8.0",
    "@types/react-dom": "^16.8.0",
    "big.js": "^5.2.2",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "rxjs": "^6.4.0",
    "scheduler": "0.14.0",
    "tslib": "^1.9.3",
    "utility-types": "^3.5.0"
  },
  "devDependencies": {
    "docz": "^1.1.0",
    "docz-theme-default": "^1.1.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.8.0",
    "rimraf": "^2.6.3",
    "typescript": "~3.4.0"
  }
}
